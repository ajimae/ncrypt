{
  "name": "@ajimae/ncrypt",
  "module": "dist/index.js",
  "version": "0.0.0-beta.0",
  "description": "a light weight javascript data encryption and decryption library",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajimae/ncrypt.git"
  },
  "bugs": {
    "url": "https://github.com/ajimae/ncrypt-js/issues"
  },
  "author": "ajimae",
  "license": "MIT",
  "keywords": [
    "ncrypt-js",
    "crypto",
    "cipher",
    "decrypt",
    "encrypt",
    "typescript",
    "ecmascript5",
    "encryption",
    "cryptojs",
    "cryptography",
    "decryption",
    "javascript-library"
  ],
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^5.2.7",
    "@types/node": "^20.11.30",
    "bun-plugin-dts": "^0.2.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "mocha": "^6.2.2",
    "mocha-istanbul": "^0.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-dts": "^1.3.12",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepare": "bun run build",
    "build": "bun build.cjs",
    "test": "nyc mocha --exit --colors test/**/*.ts",
    "dts": "./node_modules/.bin/npm-dts generate --entry index.ts --output dist/index.d.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  }
